<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:actionBarStyle">@style/MyActionBar</item>

    </style>
    <!-- ActionBar styles -->
    <style name="MyActionBar">
        <item name="android:background">#ff2968ff</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="iconGenText">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ff2968ff</item>
    </style>
    <style name="PhotoGridLayout">
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:listSelector">@null</item>
    </style>

    <style name="CustomTheme" parent="@style/Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimary">@color/primary_material_light_1</item>
    </style>

</resources>
